        inactive_style: (fg: "#ffffff", bg: "#171719", modifiers: ""),
    ),
    highlighted_item_style: (fg: "#f4d26c", modifiers: "Bold"),
    current_item_style: (fg: "#ffffff", bg: "#8B0000", modifiers: "Bold"),
    borders_style: (fg: "#8B0000", modifiers: "Bold"),
